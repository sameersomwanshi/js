<h1>JavaScript Properties</h1>
<h3 id="browserInfo"></h3>
<h3 id="screenInfo"></h3>
<h3 id="historyInfo"></h3>
<h3 id="paragraphCount"></h3>
<h3 id="replaceText">Original Text</h3>
<script>
const browserlnfo = ‘Browser Name: ${navigator.appName)}, Version: ${navigator.appVersion};
document.getElementByld('browserinfo').textContent = browserinfo;
const screenlnfo = “Screen Width: ${window.screen.width}, Screen Height: ${window.screen.height}';
document.getElementByld('screeninfo').textContent = screeninfo;
const historyinfo = "History Length: ${window.history.length}’;
document.getElementByld('historyinfo').textContent = historyinfo;
const paragraphCount = document.getElementsByTagName('p').length;
document.getElementByld('paragraphCount’).textContent = "Number of <p> tags:
${paragraphCount}’;
const replaceTextElement = document.getElementByld('replaceText');
replaceTextElement.textContent = 'Replaced Text';
</script>
