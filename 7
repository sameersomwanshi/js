JS CODE
function insertObject(arr, obj) {
arr.push(obj);
    console.log(arr);
  }
  
  
  function checkObject(arr) {
    // Check if arr is an array
    const result = Array.isArray(arr);
    if (result) {
      console.log(`${arr} is an array`);
    } else {
      console.log(`${arr} is not an array`);
    }
  }
  
 
  let array = [1, 2, 3];
let object = { x: 12, y: 8 };
  checkObject(array);
  insertObject(array, object);
  
